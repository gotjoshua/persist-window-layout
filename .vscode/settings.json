{
    "isort-formatter.useIntegratedTerminal": false,
    "autoflake-extension.check": false,
    "autoflake-extension.extension.useIntegratedTerminal": false,
    "files.associations": {
        "*.qlr": "xml",
        "*.ui": "xml",
        "*.qgs": "xml"
    },
    "editor.formatOnSave": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "mkhl.shfmt"
    },
    "editor.defaultFormatter": null,
    "[markdown]": {
        "editor.defaultFormatter": "hougesen.mdsf"
    },
    "[python]": {
        "editor.defaultFormatter": "mikoz.black-py"
    },
    "[nix]": {
        "editor.defaultFormatter": "jnoortheen.nix-ide"
    },
}